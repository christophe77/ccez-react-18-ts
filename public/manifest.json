{
	"manifest_version": 3,
	"name": "React 19 Chrome Extension",
	"version": "1.0",
	"action": {
		"default_popup": "index.html",
		"default_icon": {
			"16": "icon.jpg",
			"48": "icon.jpg",
			"128": "icon.jpg"
		}
	},
	"permissions": ["storage"],
	"background": {
		"service_worker": "background.js"
	}
}
